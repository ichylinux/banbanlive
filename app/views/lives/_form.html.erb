<%= form_for @live do |f| %>
  <table>
    <tr>
      <th><%= f.label :title %></th>
      <td><%= f.text_field :title %></td>
    </tr>
    <tr>
      <th><%= f.label :start_date %></th>
      <td><%= f.text_field :start_date %></td>
    </tr>
    <tr>
      <th><%= f.label :place %></th>
      <td><%= f.text_field :place %></td>
    </tr>
    <tr>
      <th><%= f.label :address %></th>
      <td><%= f.text_field :address %></td>
    </tr>
    <tr>
      <th><%= f.label :map_url %></th>
      <td><%= f.text_field :map_url %></td>
    </tr>
  </table>
  <%= f.submit %>
<% end %>
